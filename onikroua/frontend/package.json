    "@codetrix-studio/capacitor-google-auth": "^3.3.0",
    "@google/genai": "^1.34.0",
    "firebase": "^10.7.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.3",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
